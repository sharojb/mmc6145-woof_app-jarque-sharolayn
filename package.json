{
  "name": "woof",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "vitest",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "mongo": "npm run mongo:restart",
    "mongo:restart": "npm run mongo:stop; npm run mongo:start",
    "mongo:start": "export DB_NAME=booker && docker run --rm -d -p 27017:27017 --name=mongo-sandbox -e MONGO_INITDB_DATABASE=$DB_NAME mongo:5",
    "mongo:stop": "docker container stop mongo-sandbox",
    "mongo:shell": "docker exec -it mongo-sandbox mongosh"
  },
  "dependencies": {
    "@next/font": "^13.0.3",
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "iron-session": "^6.3.1",
    "mongoose": "^6.4.0",
    "next": "13.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@vitejs/plugin-react": "^2.2.0",
    "cross-fetch": "^3.1.5",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "jsdom": "^20.0.2",
    "msw": "^0.49.0",
    "node-mocks-http": "^1.12.1",
    "vitest": "^0.25.2"
  }
}
